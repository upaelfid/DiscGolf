@model DiscGolf.Models.RegisterModel
@{
    ViewBag.Title = "Register";
}

<hgroup class="title">

</hgroup>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <fieldset>
        <legend>Registration Form</legend>
        <ol class="uk-panel uk-panel-box uk-form" style="width:80%; margin-left:15px; ">
                        <img class="uk-margin-bottom" width="80%" src="data:image/png;base64," alt="">

            <li class="uk-form-row">
                @Html.TextBoxFor(m => m.UserName, new{ @placeholder="User Name"})
            </li>
            <li class="uk-form-row">
               @Html.PasswordFor(m => m.Password, new{ @placeholder="Password"})
            </li>
            <li class="uk-form-row">
                @Html.PasswordFor(m => m.ConfirmPassword, new{ @placeholder="Confirm Password"})
            </li>
            <li class="uk-form-row">
            <input type="submit" value="Register" class="uk-width-1-1 uk-button uk-button-primary "  style="width:80%; color:white;"/>
            </li>
        </ol>
        
    </fieldset>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
